    "require-dev": {
        "drupal/features": "^3.7",
        "webflo/drupal-core-require-dev": "8.6.16",
        "drupal/devel": "^1.2",
        "weitzman/drupal-test-traits": "dev-pass-chrome-driver-options",
        "dmore/chrome-mink-driver": "^2.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "drupal/core": "dist"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "web/libraries/swordappv2-php-library/swordappclient.php",
            "web/libraries/swordappv2-php-library/packager_mets_swap.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::placeProfile",
            "DrupalProject\\composer\\ScriptHandler::installFullcalendarLibrary",
            "command -v npm install || echo \"NPM not installed. Please install it and run composer install again.\"",
            "DrupalProject\\composer\\ScriptHandler::installBootstrapLibrary",
            "DrupalProject\\composer\\ScriptHandler::installOsToolbarLibrary",
            "DrupalProject\\composer\\ScriptHandler::placeCustomThemes",
            "./scripts/register-cs.sh",
            "./scripts/install-hooks.sh"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::placeProfile",
            "DrupalProject\\composer\\ScriptHandler::installFullcalendarLibrary",
            "command -v npm update || echo \"NPM not installed. Please install it and run composer install.\"",
            "DrupalProject\\composer\\ScriptHandler::installBootstrapLibrary",
            "DrupalProject\\composer\\ScriptHandler::installOsToolbarLibrary",
            "DrupalProject\\composer\\ScriptHandler::placeCustomThemes"
        ],
        "code-standard": [
            "./vendor/bin/phpcs --standard=Drupal --ignore=*.yml,*.md",
            "./vendor/bin/phpcs --standard=DrupalPractice"
        ],
        "code-standard-fix": [
            "./vendor/bin/phpcbf --standard=Drupal"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "source": [
                "https://raw.githubusercontent.com/drupal/drupal/{version}/{path}"
            ]
        },
        "patches": {
            "drupal/core": {
                "Make sure TestSitePath exist before creating .htkey": "https://www.drupal.org/files/issues/2246725-24.patch",
                "Views Date Filter Datetime Granularity Option": "https://www.drupal.org/files/issues/2018-12-11/views_date_filter_granularity_2868014-60.patch",
                "Add paging info to rest request": "https://www.drupal.org/files/issues/2018-06-30/2982729-viewsRestInfo.patch",
                "Getlabel() on null layout builder.": "https://www.drupal.org/files/issues/2018-07-16/2985882-entityfield-2.patch",
                "'view_path' is set to /views/ajax after second ajax request": "https://www.drupal.org/files/issues/2018-03-14/drupal-views_path_ajax-2866386-10.patch"
            },
            "drupal/group_purl": {
                "Prevent base path from interfering with purl detection": "https://www.drupal.org/files/issues/2019-04-12/group_purl.3026646-3.basepath.patch",
                "Don't attempt to redirect to subdomains by default": "https://www.drupal.org/files/issues/2019-03-05/group_purl.3036879-4.subdomain_disable.patch"
            },
            "drupal/fullcalendar": {
                "Fullcalendar incompatible with Date Recur 2.x": "https://www.drupal.org/files/issues/2019-01-09/3023612-5.patch"
            },
            "drupal/block_visibility_groups": {
                "8.6: error when deleting a block visibility group": "https://www.drupal.org/files/issues/2018-10-18/block_visibility_groups_3000396-4.patch"
            },
            "drupal/bootstrap": {
                "Unsupported operand types during providers JSON discovery #8": "https://www.drupal.org/files/issues/2019-01-24/3027569-8.x-3.x-8.patch",
                "Fix modal elements getting lost after reusing modal": "https://raw.githubusercontent.com/openscholar/openscholar/e4d27051b02cb9c92e44e76c5e9f4f4a57c2e874/patches/bootstrap.11027.footer.patch"
            },
            "drupal/mailchimp": {
                "Cache read problem in mailchimp_get_mergevars()": "https://www.drupal.org/files/issues/2019-02-13/3032674-cache-fix-for-mailchimp_get_mergevars-3.patch"
            },
            "drupal/ckeditor_bidi": {
                "Update Bidi plugin.js to latest version": "https://www.drupal.org/files/issues/2019-03-15/update_bidi_plugin_3040454_4.patch"
            },
            "ckeditor/mathjax": {
                "Mathjax cdn url config patch" : "patches/mathjax_cdnjs_config.patch"
            },
            "drupal/redirect": {
                "Handle matched redirect alter" : "patches/redirect-subscriber-hook-alter.patch"
            },
            "drupal/rng": {
                "Make ResultAccessRules::getTitle compatible": "https://github.com/dpi/rng/pull/192.patch",
                "Fixes radio count warnings in registrants.php": "https://github.com/dpi/rng/pull/194.patch"
            },
            "drupal/courier": {
                "Fixes checkboxes count warning": "https://github.com/dpi/courier/pull/48.patch"
            },
            "drupal/group": {
                "/views/ajax?_wrapper_format=drupal_ajax HTTP/1.1 - HTTP/1.1 403 Forbidden": "https://www.drupal.org/files/issues/2018-08-15/group-route-match-ajax-views-d8-2942657-11.patch",
                "Integrity constraint violation: Duplicate entry for key PRIMARY in node_access": "https://www.drupal.org/files/issues/2018-08-17/group-duplicate-node-access-entries-2892512-15.patch",
                "New group content created when node edited": "https://www.drupal.org/files/issues/2019-06-04/3059300-3.patch",
                "Prevent resaving of deleted entities": "https://raw.githubusercontent.com/openscholar/openscholar/ae879229e866cbc96d91d2b2a8273292fa26fd34/patches/group.fix_saving_deleted_entity.patch",
                "Entities identified by strings as group content": "https://www.drupal.org/files/issues/2019-05-28/group-support_string_entity_ids-2797793-162.patch"
            },
            "drupal/shorten": {
                "Update is.gd service api url": "https://www.drupal.org/files/issues/2019-07-03/update-isgd-api-3065567-2.patch"
            },
            "drupal/bibcite": {
                "Extending the supported bibcite formats.": "patches/bibcite_formats_extension.patch",
                "Editors and others roles treated as author": "https://www.drupal.org/files/issues/2019-08-01/other-roles-shown-as-authors-3006180-6.patch"
            }
        },
        "patches-ignore": {
            "weitzman/drupal-test-traits": {
                "drupal/core": {
                    "Test runs can throw exceptions due to missing .htkey file (https://www.drupal.org/project/drupal/issues/2246725#comment-12160940)": "https://www.drupal.org/files/issues/2246725-24.patch"
                }
            }
        }
    }
}
